{
  "name": "bba-website",
  "version": "1.0.0",
  "description": "BBA FinTech Landing Page",
  "type": "module",
  "scripts": {
    "generate-blog": "bun run scripts/generate-blog-data.ts",
    "dev": "bun run generate-blog && bun --hot app.ts",
    "build": "bun run generate-blog && bun run prerender",
    "prerender": "bun run scripts/prerender.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@preact/signals": "^2.5.1",
    "gray-matter": "^4.0.3",
    "marked": "^17.0.1",
    "preact": "^10.28.0",
    "preact-render-to-string": "^6.6.3",
    "preact-router": "^4.1.2"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "typescript": "^5.9.3"
  }
}